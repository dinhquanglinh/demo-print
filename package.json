        "vuetify": "^2.6.14",
        "vuex": "^3.4.0",
        "web-bluetooth": "^0.1.2"
    },
    "devDependencies": {
        "@babel/core": "^7.16.5",
        "@babel/preset-env": "^7.16.5",
        "@mdi/font": "^7.1.96",
        "@types/jest": "^24.0.19",
        "@types/node": "^17.0.10",
        "@typescript-eslint/eslint-plugin": "^4.18.0",
        "@typescript-eslint/parser": "^4.18.0",
        "@vue/cli-plugin-babel": "~4.5.0",
